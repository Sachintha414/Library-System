<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>db</servlet-name>
        <servlet-class>servlets.db</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>icalendar</servlet-name>
        <servlet-class>servlets.icalendar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>regdb</servlet-name>
        <servlet-class>servlets.regdb</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>idea</servlet-name>
        <servlet-class>idea</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>idead</servlet-name>
        <servlet-class>idead</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>db</servlet-name>
        <url-pattern>/db</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>icalendar</servlet-name>
        <url-pattern>/icalendar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>regdb</servlet-name>
        <url-pattern>/regdb</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>idea</servlet-name>
        <url-pattern>/idea</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>idead</servlet-name>
        <url-pattern>/idead</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
